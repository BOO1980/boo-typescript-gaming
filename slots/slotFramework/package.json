{
  "name": "slotframework",
  "version": "0.0.0",
  "description": "a simple slot framework",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --default",
    "test": "webpack --mode development --config webpack.config.dev.js",
    "build": "webpack --mode production --config webpack.config.prod.js",
    "buildBoth": "webpack --mode production --config webpack.config.prod.js && uglifyjs ./dist/honeypot.js -c -m -o ./dist/honeypot.min.js"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "babel-loader": "^8.1.0",
    "@babel/preset-env": "^7.14.8",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^3.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "pixi.js": "5.3.10",
    "zip-webpack-plugin": "^3.0.0"
  }
}
